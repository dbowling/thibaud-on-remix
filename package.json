{
  "name": "remix-app-template",
  "version": "0.0.1",
  "description": "Personal website built with Remix.run",
  "license": "MIT",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev",
    "postinstall": "remix setup node",
    "start": "cross-env NODE_ENV=production npm run build && remix-serve build",
    "prettier": "prettier --ignore-path .gitignore \"**/*.{js,jsx,ts,tsx,json,md,mdx,html,css}\"",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --check"
  },
  "dependencies": {
    "@remix-run/react": "^0.20.1",
    "@remix-run/serve": "^0.20.1",
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6.0.0-beta.8",
    "remix": "^0.20.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^0.20.1",
    "@tidusia/prettier-config": "^2.1.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "prettier": "^2.4.1",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": "^16"
  },
  "sideEffects": false
}
