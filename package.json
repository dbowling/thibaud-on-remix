{
  "name": "remix-app-template",
  "version": "0.0.1",
  "description": "Personal website built with Remix.run",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "build": "remix build",
    "dev": "remix dev",
    "postinstall": "remix setup node",
    "start": "cross-env NODE_ENV=production npm run tailwind:build && npm run build && remix-serve build",
    "prettier": "prettier \"**/*.{js,jsx,ts,tsx,json,md,mdx,html,css}\"",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --check",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "eslint . --fix --ignore-path .gitignore",
    "validate-typescript": "tsc --noEmit",
    "tailwind:watch": "npx tailwindcss -o app/styles/tailwind.css --watch",
    "tailwind:build": "npx tailwindcss -o app/styles/tailwind.css --minify",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@remix-run/react": "^0.20.1",
    "@remix-run/serve": "^0.20.1",
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6.0.0-beta.8",
    "remix": "^0.20.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^0.20.1",
    "@tidusia/eslint-config": "^2.0.0",
    "@tidusia/prettier-config": "^2.1.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": "^16",
    "npm": "^8"
  },
  "sideEffects": false
}
